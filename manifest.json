{
  "manifest_version": 2,
  "name": "Stack Overflow Class Linker",
  "version": "1.0.2",
  "content_scripts" : [
  	{
      "run_at" : "document_end",
  	  "matches": ["http://stackoverflow.com/*"],
  	  "js" : ["jquery-2.2.0.min.js", "classes/java8Classes.json", "classes/androidClasses.json", "findClassName.js", "content.js" ],
      "css" : ["css.css"]
  	}
  ]
}
